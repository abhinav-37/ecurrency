<body>

    <div class="container log-in-content-wrap">
        <h2>Register</h2>
        <div class="login-form mtn-15">
            <form action="/login" method="post">
                <div class="form-input-item">
                    <label for="first_name" class="sr-only">First name</label>
                    <input required type="email" id="first_name" name="username" placeholder="Email">
                </div>
                <div class="form-input-item">
                    <label for="username" class="sr-only">Email</label>
                    <input required type="email" id="username" name="username" placeholder="Email">
                </div>

                <div class="form-input-item">
                    <label for="contact_number" class="sr-only">Contact number</label>
                    <input required type="number" id="contact_number" placeholder="contact number"
                        name="contact_number">
                </div>
                <div class="form-input-item">
                    <label for="password" class="sr-only">Password</label>
                    <input required type="password" id="password" name="password" placeholder="password">
                </div>

                <div class="form-input-item">
                    <button type="submit" class="btn-submit">Login</button>
                </div>
            </form>
        </div>

        <div class="sign-up-notification">
            <p>Already Resisted? <a href="/">Login Now.</a></p>
        </div>
    </div>
    <script src="js/modernizr-3.6.0.min.js"></script>
    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/jquery-migrate-1.2.1.min.js"></script>
